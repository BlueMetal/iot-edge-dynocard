{
  "moduleContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": ""
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "microsoft/azureiotedge-agent:1.0-preview",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-hub:1.0.0-preview021",
              "createOptions": ""
            }
          }
        },
        "modules": {
          "modbus": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-modbus-tcp:1.0.2-preview",
              "createOptions": "{\"HostConfig\":{\"Devices\": [{\"PathOnHost\": \"/dev/ttyUSB0\",\"PathInContainer\": \"/dev/ttyUSB0\",\"CgroupPermissions\": \"rwm\"}]}}"
            }
          },
          "dynoCardAlertModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "bluemetal/iot_dynocard_demo_alert:latest",
              "createOptions": ""
            }
          },
          "mlAlertModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "bluemetal/iot_dynocard_demo_edgeml:latest",
              "createOptions": ""
            }
          },
          "sql": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "bluemetal/iot_dynocard_demo_edgedb:latest",
              "createOptions": "{\"Env\": [\"ACCEPT_EULA=Y\",\"MSSQL_SA_PASSWORD=Strong!Passw0rd\"],\"HostConfig\": {\"Mounts\": [{\"Target\": \"/var/opt/mssql\",\"Source\": \"sqlVolume\",\"Type\": \"volume\"}],\"PortBindings\": {\"1433/tcp\": [{\"HostPort\": \"1401\"}]}}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "modbusWrite": "FROM /messages/modules/dynoCardAlertModule/outputs/modbusWriteOutput INTO BrokeredEndpoint(\"/messages/modules/modbus/inputs/input1\")",
          "modbusToDynoCard": "FROM /messages/modules/modbus/outputs/modbusOutput INTO BrokeredEndpoint(\"/modules/dynoCardAlertModule/inputs/modbusInput\")",
          "opcToCustomHandler": "FROM /messages/modules/opc/* INTO BrokeredEndpoint(\"/modules/dynoCardAlertModule/inputs/opcInput\")",
          "sensorToAlert": "FROM /messages/modules/dynoCardAlertModule/outputs/output1 INTO BrokeredEndpoint(\"/modules/mlAlertModule/inputs/amlInput\")",
          "alertTofilterAlert": "FROM /messages/modules/mlAlertModule/outputs/amlOutput INTO BrokeredEndpoint(\"/modules/dynoCardAlertModule/inputs/alertInput\")",
          "filterToIoTHub": "FROM /messages/modules/dynoCardAlertModule/outputs/alertOutput INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "dynoCardAlertModule": {
      "properties.desired": {
        "surfaceCardConfig": {
          "timestamp": {
            "register": "35749",
            "length": "2"
          },
          "numberOfPoints": {
            "register": "35751",
            "length": "1"
          },
          "scaledMaxCardLoad": {
            "register": "35752",
            "length": "1"
          },
          "scaledMinCardLoad": {
            "register": "35753",
            "length": "1"
          },
          "strokeLength": {
            "register": "35754",
            "length": "1"
          },
          "strokePeriod": {
            "register": "35755",
            "length": "1"
          },
          "pointArrayStartRegister": {
            "register": "35756",
            "length": "2"
          }
        },
        "pumpCardConfig": {
          "timestamp": {
            "register": "36156",
            "length": "2"
          },
          "scaledMaxCardLoad": {
            "register": "36158",
            "length": "1"
          },
          "scaledMinCardLoad": {
            "register": "36159",
            "length": "1"
          },
          "numberOfPoints": {
            "register": "36160",
            "length": "1"
          },
          "grossStroke": {
            "register": "36161",
            "length": "1"
          },
          "netStroke": {
            "register": "36162",
            "length": "1"
          },
          "pumpFillage": {
            "register": "36163",
            "length": "1"
          },
          "fluidLoad": {
            "register": "36164",
            "length": "1"
          },
          "pointArrayStartRegister": {
            "register": "36165",
            "length": "2"
          }
        },
        "sqlConnectionString": "Data Source=tcp:sql,1433;Initial Catalog=db4cards;User Id=SA;Password=Strong!Passw0rd;TrustServerCertificate=False;Connection Timeout=30;"
      }
    },
    "modbus": {
      "properties.desired": {
        "PublishInterval": "10000",
        "SlaveConfigs": {
          "Slave01": {
            "SlaveConnection": "/dev/ttyUSB0",
            "RetryCount" : "10",
            "RetryInterval" : "50",
            "BaudRate": "9600",
            "DataBits": "8",
            "StopBits": "1",
            "Parity": "NONE",
            "FlowControl": "NONE",
            "HwId": "Pump1-DynoCard",
            "Operations": {
              "Op01": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "35749",
                "Count": "57",
                "DisplayName": "SurfaceCard-Batch1"
              },
              "Op02": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "35805",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch2"
              },
              "Op03": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "35855",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch3"
              },
              "Op04": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "35905",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch4"
              },
              "Op05": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "35955",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch5"
              },
              "Op06": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36005",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch6"
              },
              "Op07": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36055",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch7"
              },
              "Op08": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36105",
                "Count": "50",
                "DisplayName": "SurfaceCard-Batch8"
              },
              "Op09": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36156",
                "Count": "59",
                "DisplayName": "PumpCard-Batch1"
              },
              "Op10": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36214",
                "Count": "50",
                "DisplayName": "PumpCard-Batch2"
              },
              "Op11": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36264",
                "Count": "50",
                "DisplayName": "PumpCard-Batch3"
              },
              "Op12": {
                "PollingInterval": "1500",
                "UnitId": "1",
                "StartAddress": "36314",
                "Count": "50",
                "DisplayName": "PumpCard-Batch4"
              }
            }
          }
        }
      }
    }
  }
}