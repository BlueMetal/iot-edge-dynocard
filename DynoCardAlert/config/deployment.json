{
  "moduleContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": ""
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "microsoft/azureiotedge-agent:1.0-preview",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-hub:1.0.0-preview021",
              "createOptions": ""
            }
          }
        },
        "modules": {
          "modbus": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-modbus-tcp:1.0.2-preview",
              "createOptions": ""
            }
          },
          "dynoCardAlertModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "localhost:5000/dynocardalertmodule:0.0.1-amd64.debug",
              "createOptions": ""
            }
          },
          "sql": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/mssql-server-linux:2017-latest",
              "createOptions": "{\"Env\": [\"ACCEPT_EULA=Y\",\"MSSQL_SA_PASSWORD=Strong!Passw0rd\"],\"HostConfig\": {\"Mounts\": [{\"Target\": \"/var/opt/mssql\",\"Source\": \"sqlVolume\",\"Type\": \"volume\"}],\"PortBindings\": {\"1433/tcp\": [{\"HostPort\": \"1401\"}]}}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "modbusToDynoCard": "FROM /messages/modules/modbus/outputs/modbusOutput INTO BrokeredEndpoint(\"/modules/dynoCardAlertModule/inputs/modbusInput\")",
          "sensorToFakeAlert": "FROM /messages/modules/dynoCardAlertModule/outputs/output1 INTO BrokeredEndpoint(\"/modules/fakeAlertModule/inputs/input1\")",
          "fakeAlertTofilterAlert": "FROM /messages/modules/fakeAlertModule/outputs/output1 INTO BrokeredEndpoint(\"/modules/dynoCardAlertModule/inputs/alertInput\")",
          "filterToIoTHub": "FROM /messages/modules/dynoCardAlertModule/outputs/alertOutput INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "dynoCardAlert": {
        "properties.desired": {
          "timestamp": {
            "register": "40001",
            "length": "2"
          },
          "numberOfPoints": {
            "register": "40003",
            "length": "1"
          },
          "scaledMaxCardLoad": {
            "register": "40004",
            "length": "1"
          },
          "scaledMinCardLoad": {
            "register": "40005",
            "length": "1"
          },
          "strokeLength": {
            "register": "40006",
            "length": "1"
          },
          "strokePeriod": {
            "register": "40007",
            "length": "1"
          },
          "pointArrayStartRegister": {
            "register": "40008",
            "length": "2"
          },
          "sqlConnectionString": "Data Source=tcp:sql,1433;Initial Catalog=db4cards;User Id=SA;Password=Strong!Passw0rd;TrustServerCertificate=False;Connection Timeout=30;"
        }
    },
    "modbus": {
      "properties.desired": {
        "PublishInterval": "2000",
        "SlaveConfigs": {
          "Slave01": {
            "SlaveConnection": "40.71.220.129",
            "HwId": "Pump1-DynoCard",
            "Operations": {
              "Op01": {
                "PollingInterval": "3000",
                "UnitId": "1",
                "StartAddress": "40001",
                "Count": "250",
                "DisplayName": "SurfaceCard"
              },
              "Op02": {
                "PollingInterval": "-1",
                "UnitId": "2",
                "StartAddress": "40407",
                "Count": "0",
                "DisplayName": "PumpCard"
              }
            }
          }
        }
      }
    }
  }
}